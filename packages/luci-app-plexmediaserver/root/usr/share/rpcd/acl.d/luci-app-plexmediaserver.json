{
  "luci-app-plexmediaserver": {
    "description": "Grant access to Plex Media Server control commands",
    "read": {
      "uci": [
        "plexmediaserver"
      ]
    },
    "write": {
      "uci": [
        "plexmediaserver"
      ],
      "file": {
        "/etc/init.d/plexmediaserver": [ "exec" ],
        "/usr/bin/pgrep": [ "exec" ],
        "/bin/sh": [ "exec" ],
        "/sbin/ip": [ "exec" ],
        "/sbin/logread": [ "exec" ]
      }
    }
  }
}